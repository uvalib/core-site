{
  "name": "core-site",
  "version": "1.0.0",
  "description": "## Prerequisites",
  "main": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "bower install; ./import-pages.js; npm run images; polymer build; ./package-build.sh;",
    "deploy-static-pages": "rsync -avh --delete -e 'ssh -i $HOME/.ssh/id_rsa' build/es6-unbundled/data/pages/ static01.lib.virginia.edu:/lib_content47/static/drupalPages;",
    "deploy": "rsync -avh --delete -e 'ssh -i $HOME/.ssh/id_rsa' build/es6-unbundled/ www14.lib.virginia.edu:/wwwstatic;",
    "images": "./import-files.js; ./imagemin-files.js;",
    "images-lite": "./import-files-lite.js; ./imagemin-files.js;",
    "images-deploy": "npm run deploy; rsync -avh -e 'ssh -i $HOME/.ssh/id_rsa' files/ www14.lib.virginia.edu:/wwwstatic/files; rsync -avh --delete -e 'ssh -i $HOME/.ssh/id_rsa' files-comp/ www14.lib.virginia.edu:/wwwstatic/files-comp",
    "quick-page-asset-update": "./import-pages.js; npm run images-lite; rsync -avh --delete -e 'ssh -i $HOME/.ssh/id_rsa' data/pages/ static01.lib.virginia.edu:/lib_content47/static/drupalPages;; rsync -avh -e 'ssh -i $HOME/.ssh/id_rsa' data/ www14.lib.virginia.edu:/wwwstatic/data;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uvalib/core-site.git"
  },
  "author": "Doug Chestnut <dougchestnut@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uvalib/core-site/issues"
  },
  "homepage": "https://github.com/uvalib/core-site#readme",
  "dependencies": {
    "bower": "^1.8.2",
    "cheerio": "^1.0.0-rc.2",
    "image-downloader": "^3.3.0",
    "imagemin": "^5.3.1",
    "imagemin-jpeg-recompress": "^5.1.0",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^5.0.1",
    "imagemin-webp": "^4.1.0",
    "make-dir": "^1.1.0",
    "moment": "^2.22.2",
    "mustache": "^2.3.0",
    "node-wget": "^0.4.2",
    "polymer-cli": "^1.5.7",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "sanitize-filename": "^1.6.1",
    "webp-converter": "^2.1.6"
  },
  "devDependencies": {
    "axe-core": "^3.0.0-beta.1"
  }
}
